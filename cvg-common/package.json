{
  "name": "cvg-common",
  "version": "1.0.0",
  "description": "Common types and utilities for CVG processing",
  "type": "module",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/index.js",
    "types": "./index.d.ts",
    "default": "./dist/index.js"
  },
  "repository": "git://github.com/novafex/cvg.git",
  "author": "Chris Pikul <ChrisPikul510@gmail.com>",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch"
  },
  "devDependencies": {
    "tsup": "^7.2.0"
  },
  "tsup": {
    "entry": ["src/index.ts"],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "target": "node18.15",
    "dts": true,
    "format": ["esm", "cjs"],
    "legacyOutput": true
  }
}
